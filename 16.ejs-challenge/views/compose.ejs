<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="form-group">
        <label for="titleTxt">Title</label>
        <input type="text" name="titleTxt" class="form-control">
        <label for="postTxt">Post</label>
        <textarea name="postTxt" id="" cols="30" rows="5" class="form-control"></textarea>

    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>





